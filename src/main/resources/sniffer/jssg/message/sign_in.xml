<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="answer">
    <protocol id="0x00240102" alias="signInDaysInThisMonth">
        <toClient desc="通知客户端本月已签到的日期和领奖的情况">
            <param fieldName="days" tag="1" fieldType="int32" repeated="true" /><!--签到的日期 -->
            <param fieldName="types" tag="2" fieldType="int32" repeated="true" /><!--已领取奖励的类型,类型指累计签到可领取奖励的天数 -->
        </toClient>
    </protocol>
    <protocol id="0x00240202" alias="signIn">
        <toClient desc="每日签到返回">
            <param fieldName="result" tag="1" fieldType="bool" />
        </toClient>
        <toServer desc="每日签到" class="com.kueem.webgame.game.action.game.SignInAction" method="signIn">
        </toServer>
    </protocol>
    <protocol id="0x00240302" alias="reward">
        <toClient desc="领取累计签到奖励返回">
            <param fieldName="result" tag="1" fieldType="bool" />
        </toClient>
        <toServer desc="领取累计签到奖励" class="com.kueem.webgame.game.action.game.SignInAction" method="reward">
            <param fieldName="type" tag="1" fieldType="int32" />
        </toServer>
    </protocol>
</config>