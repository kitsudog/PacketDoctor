<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="manage">
    <protocol id="0x00000108" alias="dailyReset">
        <toClient desc="每日重置协议返回">
            <param fieldName="result" tag="1" fieldType="int32" desc="重置返回值，1：重置成功，0：重置失败" />
        </toClient>
        <toServer desc="每日重置协议" class="com.kueem.webgame.game.action.manage.ManageAction" method="dailyReset">
            <param fieldName="ignore" tag="1" fieldType="bool" desc="是否忽略已重置的情况" />
        </toServer>
    </protocol>
    <protocol id="0x00000208" alias="maintain">
        <toClient desc="服务器挂维护返回">
            <param fieldName="msg" tag="1" fieldType="string" desc="返回消息" />
        </toClient>
        <toServer desc="服务器挂维护" class="com.kueem.webgame.game.action.manage.ManageAction" method="maintain">
        </toServer>
    </protocol>
    <protocol id="0x00000308" alias="running">
        <toClient desc="服务器恢复运行返回">
            <param fieldName="msg" tag="1" fieldType="string" desc="返回消息" />
        </toClient>
        <toServer desc="服务器恢复运行" class="com.kueem.webgame.game.action.manage.ManageAction" method="running">
        </toServer>
    </protocol>
    <protocol id="0x00000408" alias="shutdown">
        <toClient desc="服务器关闭前准备返回">
            <param fieldName="step" tag="1" fieldType="int32" desc="返回步骤" />
            <param fieldName="msg" tag="2" fieldType="string" desc="返回消息 " />
        </toClient>
        <toServer desc="服务器关闭前准备" class="com.kueem.webgame.game.action.manage.ManageAction" method="shutdown">
        </toServer>
    </protocol>
</config>