<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="relationship">

	<!-- 用户关系模块 -->
	<protocol id="0x00060102" alias="findUserRelations">
		<toServer desc="请求同步用户好友信息"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="findUserRelations">
		</toServer>
	</protocol>


	<protocol id="0x00060202" alias="findUserRelationsResult">
		<toClient desc="群获取用户好友数据" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserRelationshipBuf.UserRelationship">
		</toClient>
	</protocol>


	<protocol id="0x00060302" alias="search">
		<toServer desc="搜索好友（从好友处搜索）"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="search">
			<param fieldName="keyword" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<!-- 搜索好友结果 ID 名称 等级 阵营 职业 性别 是否在线 -->
	<protocol id="0x00060402" alias="searchResult">
		<toClient desc="搜索好友结果（从好友处搜索）">
			<param fieldName="uid" tag="1" fieldType="string" />
			<param fieldName="name" tag="2" fieldType="string" />
			<param fieldName="level" tag="3" fieldType="int32" />
			<param fieldName="group" tag="4" fieldType="int32" />
			<param fieldName="job" tag="5" fieldType="int32" />
			<param fieldName="gender" tag="6" fieldType="string" />
			<param fieldName="online" tag="7" fieldType="bool" />
			<param fieldName="vip_level" tag="8" fieldType="int32" />
			<param fieldName="vip_expire" tag="9" fieldType="int64" />
		</toClient>
	</protocol>


	<protocol id="0x00060502" alias="applyFriend">
		<toServer desc="申请加为好友"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="applyFriend">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<!-- 1 请求者ID 2请求者名称 3 头像 -->
	<protocol id="0x00060602" alias="applyFriendNotice">
		<toClient desc="好友申请通知">
			<param fieldName="uid" tag="1" fieldType="string" />
			<param fieldName="name" tag="2" fieldType="string" />
			<param fieldName="job" tag="3" fieldType="int32" />
			<param fieldName="camp" tag="4" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00060702" alias="agreeFriend">
		<toServer desc="同意好友申请"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="agreeFriend">
			<param fieldName="uids" tag="1" fieldType="string" repeated="true" />
		</toServer>
	</protocol>

	<protocol id="0x00060802" alias="findUserRelationsData">
		<toClient desc="群获取用户好友数据" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserRelationshipBuf.UserRelationship">
		</toClient>
	</protocol>

	<protocol id="0x00060902" alias="deleteFriend">
		<toServer desc="删除好友"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="deleteFriend">
			<param fieldName="uids" tag="1" fieldType="string" repeated="true" />
		</toServer>
	</protocol>

	<!-- 1 类别, 2 id -->
	<protocol id="0x00060A02" alias="deleteFriendResult">
		<toClient desc="关系减少">
			<param fieldName="type" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="uid" tag="2" fieldType="string" repeated="true" />
		</toClient>
	</protocol>

	<protocol id="0x00060B02" alias="moveToBlockList">
		<toServer desc="加入黑名单"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="moveToBlockList">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00060C02" alias="deleteFromBlockList">
		<toServer desc="从黑名单删除"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="deleteFromBlockList">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00060D02" alias="moveToClose">
		<toServer desc="加为密友"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="moveToClose">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00060E02" alias="moveToFriend">
		<toServer desc="从密友移动到好友"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="moveToFriend">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<!-- 1 type 1变为密友 2变为好友, 2 targetID -->
	<protocol id="0x00060F02" alias="closeChange">
		<toClient desc="密友变化">
			<param fieldName="type" tag="1" fieldType="int32" />
			<param fieldName="targetId" tag="2" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00061002" alias="deleteFromEnemy">
		<toServer desc="删除仇人"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="deleteFromEnemy">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>
	<protocol id="0x00061102" alias="bless">
		<toServer desc="祝福"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="bless">
			<param fieldName="type" tag="1" fieldType="int32" />
			<param fieldName="uids" tag="2" fieldType="string" repeated="true" />
		</toServer>
	</protocol>


	<!-- 1、今日祝福数 2、祝福者ID -->
	<protocol id="0x00061202" alias="receiveBless">
		<toClient desc="收到好友祝福">
			<param fieldName="bless" tag="1" fieldType="int32" />
			<param fieldName="fid" tag="2" fieldType="string" />
		</toClient>
	</protocol>


	<protocol id="0x00061302" alias="reward">
		<toServer desc="领奖"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="reward">
		</toServer>
	</protocol>

	<!-- 领奖操作是否成功 -->
	<protocol id="0x00061402" alias="rewardResult">
		<toClient desc="领奖操作是否成功">
			<param fieldName="success" tag="1" fieldType="bool" />
		</toClient>
	</protocol>


	<protocol id="0x00061502" alias="searchForChat">
		<toServer desc="搜索好友（从聊天处搜索）"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="searchForChat">
			<param fieldName="keyword" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<!-- 搜索好友结果 ID 名称 等级 阵营 职业 性别 是否在线 -->
	<protocol id="0x00061602" alias="searchForChatResult">
		<toClient desc="搜索好友结果（从聊天处搜索）">
			<param fieldName="uid" tag="1" fieldType="string" />
			<param fieldName="name" tag="2" fieldType="string" />
			<param fieldName="level" tag="3" fieldType="int32" />
			<param fieldName="group" tag="4" fieldType="int32" />
			<param fieldName="job" tag="5" fieldType="int32" />
			<param fieldName="gender" tag="6" fieldType="string" />
			<param fieldName="online" tag="7" fieldType="bool" />
			<param fieldName="vip_level" tag="8" fieldType="int32" />
			<param fieldName="vip_expire" tag="9" fieldType="int64" />
		</toClient>
	</protocol>

	<protocol id="0x00061702" alias="relationshipUpdate">
		<toClient desc="群获取用户好友数据" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserRelationshipBuf.UserRelationship.User">
		</toClient>
	</protocol>

	<protocol id="0x00061802" alias="syncOtherInfo">
		<toServer desc="请求玩家的详细信息"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="syncOtherInfo">
			<param fieldName="targetId" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00061902" alias="friendUserInfo">
		<toClient desc="获取好友的详细信息" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.FriendUserInfoBuff.FriendUserInfo">
		</toClient>
	</protocol>

	<protocol id="0x00061A02" alias="randomUsers">
		<toServer desc="随机一批用户"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="randomUsers">
		</toServer>
	</protocol>

	<protocol id="0x00061B02" alias="randomUsersInfo">
		<toClient desc="返回随机出来的用户信息" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserProfileBuffers.RandomUserList">
		</toClient>
	</protocol>

	<protocol id="0x00061C02" alias="applyFriendOneKey">
		<toServer desc="一键申请好友"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="applyFriendOneKey">
			<param fieldName="uids" tag="1" fieldType="string" repeated="true"
				desc="要申请为好友的ID列表" />
		</toServer>
	</protocol>

	<protocol id="0x00061D02" alias="friendRecommandCount">
		<toClient desc="好友推荐的次数返回">
			<param fieldName="count" tag="1" fieldType="int32" desc="好友推荐的次数" />
		</toClient>
		<toServer desc="好友推荐的次数"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="friendRecommandCount">
		</toServer>
	</protocol>

	<protocol id="0x00061F02" alias="otherEquipWashInfo">
		<toClient desc="返回其他角色的洗炼属性" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.FriendUserInfoBuff.FriendUserInfo.FriendEquipWashPractice">
		</toClient>
		<toServer desc="查看其他角色的洗炼属性"
			class="com.kueem.webgame.game.action.game.RelationshipAction" method="requestOtherUserEquipWash">
			<param fieldName="targetId" tag="1" fieldType="string" desc="其他角色的id" />
			<param fieldName="ueId" tag="2" fieldType="int32" desc="要查看的装备的id" />
		</toServer>
	</protocol>

	<protocol id="0x00062002" alias="syncFriendClose">
		<toClient desc="同步与单个好友的亲密度">
			<param fieldName="fid" tag="1" fieldType="string" desc="好友id" />
			<param fieldName="close" tag="2" fieldType="int32" desc="亲密度" />
		</toClient>
	</protocol>


	<protocol id="0x00062102" alias="syncNotFriendInTeam">
		<toClient desc="队伍解散后，同步不是好友的玩家信息给离开房间的人">
			<param fieldName="uid" tag="1" fieldType="string" repeated="true" />
			<param fieldName="uanme" tag="2" fieldType="string" repeated="true" />
			<param fieldName="job" tag="3" fieldType="int32" repeated="true" />
			<param fieldName="camp" tag="4" fieldType="int32" repeated="true" />
		</toClient>
	</protocol>
</config>