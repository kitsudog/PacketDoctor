<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="scene">
	<protocol id="0x00090102" alias="requestEnter">
		<toClient desc="请求进入一个场景返回">
			<param fieldName="mapId" tag="1" fieldType="int32" />
		</toClient>
		<toServer desc="请求进入一个场景"
			class="com.kueem.webgame.game.action.game.SceneAction" method="requestEnter">
			<param fieldName="mapId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00090202" alias="enter">
		<toServer desc="进入地图场景"
			class="com.kueem.webgame.game.action.game.SceneAction" method="enter">
			<param fieldName="mapId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00090302" alias="playerEnter">
		<toClient desc="场景中的玩家进入视野" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserProfileBuffers.UserProfileProto">
		</toClient>
	</protocol>
	<protocol id="0x00090402" alias="monsterEnter">
		<toClient desc="场景中的怪物进入视野" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.MonsterBuffers.MonsterProto">
		</toClient>
	</protocol>
	<protocol id="0x00090502" alias="leave">
		<toClient desc="场景中的玩家离开视野">
			<param fieldName="id" tag="1" fieldType="string" />
			<param fieldName="type" tag="2" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00090602" alias="flopBox">
		<toClient desc="场景中宝箱掉落" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.FlopBoxBuffers.FlopBoxProto">
		</toClient>
	</protocol>
	<protocol id="0x00090702" alias="removeBox">
		<toClient desc="移除场景中的宝箱">
			<param fieldName="id" tag="1" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00090802" alias="pickFlopBox">
		<toServer desc="拾取物品"
			class="com.kueem.webgame.game.action.game.SceneAction" method="pickFlopBox">
			<param fieldName="flopBoxId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00090902" alias="openAreaMap">
		<toClient desc="开启区域地图">
			<param fieldName="areaId" tag="1" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00090A02" alias="exitScene">
		<toServer desc="请求离开场景"
			class="com.kueem.webgame.game.action.game.SceneAction" method="exitScene">
			<param fieldName="pos" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00090B02" alias="getMapsEnterNum">
		<toClient desc="返回指定区域内场景进入次数和对应评分">
			<param fieldName="mapIdList" tag="1" fieldType="int32"
				repeated="true" desc="场景" />
			<param fieldName="numList" tag="2" fieldType="int32" repeated="true"
				desc="进入次数" />
			<param fieldName="score" tag="3" fieldType="int32" repeated="true"
				desc="对应评分" />
			<param fieldName="area_id" tag="4" fieldType="int32" repeated="false"
				desc="区域id" />
			<param fieldName="gainOrNot" tag="5" fieldType="bool" repeated="false"
				desc="是否领取过通关奖励" />
		</toClient>
		<toServer desc="获取指定区域内场景进入次数和对应评分"
			class="com.kueem.webgame.game.action.game.SceneAction" method="getMapsEnterNum">
			<param fieldName="area_id" tag="1" fieldType="int32" repeated="false" />
		</toServer>
	</protocol>
	<protocol id="0x00090C02" alias="pickFlopItem">
		<toServer desc="拾取掉落物品箱子中的单个物品"
			class="com.kueem.webgame.game.action.game.SceneAction" method="pickFlopItem">
			<param fieldName="flopBoxId" tag="1" fieldType="int32" />
			<param fieldName="flopItemId" tag="2" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00090D02" alias="removeItem">
		<toClient desc="移除场景中宝箱里面单一物品">
			<param fieldName="boxId" tag="1" fieldType="int32" />
			<param fieldName="itemId" tag="2" fieldType="int32" />
		</toClient>
	</protocol>


	<protocol id="0x00090E02" alias="finishPickBox">
		<toServer desc="拾取掉落宝箱完成"
			class="com.kueem.webgame.game.action.game.SceneAction" method="finishPickBox">
		</toServer>
	</protocol>
	<protocol id="0x00090F02" alias="gateEnter">
		<toClient desc="传送点进入视野 ">
			<param fieldName="id" tag="1" fieldType="int32" />
			<param fieldName="x" tag="2" fieldType="int32" />
			<param fieldName="y" tag="3" fieldType="int32" />
			<param fieldName="mapid" tag="4" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00091002" alias="getLastDuplicateInTown">
		<toClient desc="获取所通关副本中最高等级的副本">
			<param fieldName="duplicate_id" tag="1" fieldType="int32" />
		</toClient>
		<toServer desc="获取所通关副本中最高等级的副本"
			class="com.kueem.webgame.game.action.game.SceneAction" method="getLastDuplicate">
		</toServer>
	</protocol>
	<protocol id="0x00091102" alias="caijiDropItems">
		<toClient desc="采集掉落物品">
			<param fieldName="relId" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="type" tag="2" fieldType="int32" repeated="true" />
			<param fieldName="num" tag="3" fieldType="int32" repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x00091202" alias="gainAreaReward">
		<toServer desc="领取区域奖励"
			class="com.kueem.webgame.game.action.game.SceneAction" method="gainAreaReward">
			<param fieldName="area_id" tag="1" fieldType="int32" repeated="false" />
		</toServer>
	</protocol>


	<protocol id="0x00091302" alias="generalEnter">
		<toClient desc="场景中的宠物进入视野" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserGeneralInfoProtobuf.UserGeneralInfoProto.UserGeneralProto">
		</toClient>
	</protocol>

    
	<protocol id="0x00091402" alias="getUserMapInfo">
		<toServer desc="获取对应的ID集合的地图信息"
			class="com.kueem.webgame.game.action.game.SceneAction" method="getUserMapInfo">
			<param fieldName="maps" tag="1" fieldType="int32" repeated="true"
				desc="副本的ID集合" />
		</toServer>
	</protocol>

	<protocol id="0x00091502" alias="fetchAreaAchievement">
		<toClient desc="获取区域的成就数据">
			<param fieldName="areaId" tag="1" fieldType="int32" desc="区域ID" />
			<param fieldName="achievementId" tag="2" fieldType="int32"
				repeated="true" desc="成就ID" />
			<param fieldName="achievementValue" tag="3" fieldType="int32"
				repeated="true" desc="成就进度" />
			<param fieldName="achievementValue" tag="4" fieldType="int32"
				repeated="true" desc="成就是否已领奖 1 已经领奖 0 未领奖" />
		</toClient>

		<toServer desc="获取区域的成就数据"
			class="com.kueem.webgame.game.action.game.SceneAction" method="fetchAreaAchievement">
			<param fieldName="area_id" tag="1" fieldType="int32" desc="区域ID" />
		</toServer>
	</protocol>

	<protocol id="0x00091602" alias="rewardAreaAchievement">
		<toServer desc="领取区域通关的成就"
			class="com.kueem.webgame.game.action.game.SceneAction" method="rewardAreaAchievement">
			<param fieldName="area_id" tag="1" fieldType="int32" desc="区域ID" />
		</toServer>
	</protocol>

	<protocol id="0x00091702" alias="obtainQunyinzhengLayer">
		<toClient desc="获取区域的成就数据">
			<param fieldName="layer" tag="1" fieldType="int32" desc="层数" />
		</toClient>
		<toServer desc="获取群英阵今日层数"
			class="com.kueem.webgame.game.action.game.SceneAction" method="obtainQunyinzhengLayer">
			<param fieldName="firstMapId" tag="1" fieldType="int32" desc="第一个地图的ID" />
		</toServer>
	</protocol>
</config>