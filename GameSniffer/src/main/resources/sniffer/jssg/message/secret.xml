<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="secret">
	<protocol id="0x001E0102" alias="random">
		<toServer desc="秘境随机宝物"
			class="com.kueem.webgame.game.action.game.SecretAction" method="random">
			<param fieldName="tab" tag="1" fieldType="int32" />
			<param fieldName="type" tag="2" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x001E0202" alias="result">
		<toClient desc="秘境随机宝物返回">
			<param fieldName="itemStr" tag="1" fieldType="string" repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x001E0302" alias="sync">
		<toServer desc="宝物快报同步"
			class="com.kueem.webgame.game.action.game.SecretAction" method="sync">
		</toServer>
	</protocol>
	<protocol id="0x001E0402" alias="syncResult">
		<toClient desc="宝物快报同步返回">
			<param fieldName="record" tag="1" fieldType="string" repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x001E0502" alias="syncAdd">
		<toClient desc="宝物快报添加">
			<param fieldName="record" tag="1" fieldType="string" repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x001E0602" alias="pick">
		<toServer desc="背包提取"
			class="com.kueem.webgame.game.action.game.SecretAction" method="pick">
			<param fieldName="index" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x001E0702" alias="pickAll">
		<toServer desc="背包全部提取"
			class="com.kueem.webgame.game.action.game.SecretAction" method="pickAll">
		</toServer>
	</protocol>
	<protocol id="0x001E0802" alias="syncSecretBag">
		<toClient desc="同步整个背包数据" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.UserBagProtobuf.UserBag">
		</toClient>
	</protocol>
</config>
