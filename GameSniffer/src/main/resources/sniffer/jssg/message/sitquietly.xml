<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="sitquietly">
	<protocol id="0x00210102" alias="enterSingle">
		<toServer desc="进入修行状态"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="enterSingle">
		</toServer>
	</protocol>
	<protocol id="0x00210202" alias="syncEnterSingle">
		<toClient desc="返回进入修行状态">
			<param fieldName="indexs" tag="1" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00210302" alias="enterDouble">
		<toServer desc="同意双修同时进入双修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="enterDouble">
			<param fieldName="otherHostId" tag="1" fieldType="string" />
		</toServer>
	</protocol>
	<protocol id="0x00210402" alias="syncEnterDouble">
		<toClient desc="返回进入双修状态">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="uid" tag="2" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00210502" alias="lingqiSpecial">
		<toServer desc="进入灵气专修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="lingqiSpecial">
		</toServer>
	</protocol>

	<protocol id="0x00210602" alias="syncLingqiSpecial">
		<toClient desc="返回进入灵气专修状态">
			<param fieldName="indexs" tag="1" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00210702" alias="inviteDoubleSit">
		<toServer desc="邀请双修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="inviteDoubleSit">
			<param fieldName="inviteDoubleUser" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00210802" alias="syncInviteDoubleUser">
		<toClient desc="返回邀请双修提示　成功、失败、双修中，屏蔽">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="hostId" tag="2" fieldType="string" />
			<param fieldName="name" tag="3" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00210902" alias="syncRequestDoubleSit">
		<toClient desc="发送双修邀请　　是否同意进行双修　返回 server 0x00210702">
			<param fieldName="hostId" tag="1" fieldType="string" />
			<param fieldName="name" tag="2" fieldType="string" />
			<param fieldName="comp" tag="3" fieldType="int32" />
			<param fieldName="level" tag="4" fieldType="int32" />
		</toClient>
	</protocol>


	<protocol id="0x00211002" alias="breakSitQuietly">
		<toServer desc="打断修行"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="breakSitQuietly">
		</toServer>
	</protocol>
	<protocol id="0x00211102" alias="syncBreakSitQuietly">
		<toClient desc="双修奖励结果">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="minute" tag="2" fieldType="int32" />
			<param fieldName="addExp" tag="3" fieldType="int64" />
			<param fieldName="doubleAddExp" tag="4" fieldType="int64" />
			<param fieldName="addAura" tag="5" fieldType="int64" />
			<param fieldName="doubleAddAura" tag="6" fieldType="int64" />
			<param fieldName="otherId" tag="7" fieldType="string" />
			<param fieldName="otherName" tag="8" fieldType="string" />
			<param fieldName="job" tag="9" fieldType="int32" />
			<param fieldName="camp" tag="10" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00211202" alias="cancelRuraSpecial">
		<toServer desc="取消灵修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="cancelRuraSpecial">
		</toServer>
	</protocol>

	<protocol id="0x00211302" alias="syncCancelRuraSpecial">
		<toClient desc="取消灵修返回状态">
			<param fieldName="state" tag="1" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00211402" alias="searchHalfSit">
		<toServer desc="查找可同修的玩家"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="searchHalfSit">
		</toServer>
	</protocol>



	<protocol id="0x00211502" alias="syncSearchHalfSit">
		<toClient desc="查找可同修的玩家返回">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="camp" tag="2" fieldType="int32" repeated="true" />
			<param fieldName="uid" tag="3" fieldType="string" repeated="true" />
			<param fieldName="uname" tag="4" fieldType="string" repeated="true" />
			<param fieldName="level" tag="5" fieldType="int32" repeated="true" />
		</toClient>
	</protocol>

	<protocol id="0x00211602" alias="searchHalfByName">
		<toServer desc="以名字查在线同修的人"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="searchHalfByName">
			<param fieldName="hostName" tag="1" fieldType="string" />
		</toServer>
	</protocol>



	<protocol id="0x00211802" alias="cancleInviteDoubleUser">
		<toClient desc="取消双修邀请">
			<param fieldName="hostid" tag="1" fieldType="string" />
			<param fieldName="hostName" tag="2" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00211902" alias="refuseDoubleInviter">
		<toServer desc="拒绝双修邀请"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="refuseDoubleInviter">
			<param fieldName="inviteDoubleUser" tag="1" fieldType="string" />
		</toServer>
	</protocol>

	<protocol id="0x00212002" alias="syncRefuseDoubleInviter">
		<toClient desc="拒绝双修邀请提示">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="camp" tag="2" fieldType="int32" />
			<param fieldName="hostid" tag="3" fieldType="string" />
			<param fieldName="uname" tag="4" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00212102" alias="cancelAndEnterDouble">
		<toServer desc="取消打坐　并同意双修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="cancelAndEnterDouble">
			<param fieldName="inviteDoubleUser" tag="1" fieldType="string" />
		</toServer>
	</protocol>


	<protocol id="0x00212202" alias="agreeEnterDouble">
		<toServer desc="同意双修"
			class="com.kueem.webgame.game.action.game.SitQuietlyAction" method="agreeEnterDouble">
			<param fieldName="inviteDoubleUser" tag="1" fieldType="string" />
		</toServer>
	</protocol>


	<protocol id="0x00212302" alias="broadDoubleState">
		<toClient desc="广播xxx 和xxx 双修状态 ">
			<param fieldName="state" tag="1" fieldType="int32" />
			<param fieldName="nameA" tag="2" fieldType="string" />
			<param fieldName="nameB" tag="3" fieldType="string" />
		</toClient>
	</protocol>

</config>
