<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="gem">
    <!--宝石系统 -->
    <protocol id="0x00130102" alias="hole">
        <toServer desc="装备打孔" class="com.kueem.webgame.game.action.game.GemAction" method="hole">
            <param fieldName="ueid" tag="1" fieldType="int32" />
            <param fieldName="is_bind_first" tag="2" fieldType="bool" />
            <param fieldName="auto_buy" tag="3" fieldType="bool" />
        </toServer>
    </protocol>
    <protocol id="0x00130202" alias="inlay">
        <toServer desc="宝石镶嵌" class="com.kueem.webgame.game.action.game.GemAction" method="inlay">
            <param fieldName="ueid" tag="1" fieldType="int32" />
            <param fieldName="poses" tag="2" fieldType="int32" repeated="true" />
            <param fieldName="indexList" tag="3" fieldType="int32" repeated="true" />
            <param fieldName="is_bind_first" tag="4" fieldType="bool" />
            <param fieldName="auto_buy" tag="5" fieldType="bool" />
        </toServer>
    </protocol>
    <protocol id="0x00130302" alias="remove">
        <toServer desc="宝石摘除" class="com.kueem.webgame.game.action.game.GemAction" method="remove">
