            <param fieldName="numList" tag="2" fieldType="int32" repeated="true" />
            <param fieldName="attributeList" tag="3" fieldType="int32" repeated="true" />
        </toServer>
    </protocol>
    <protocol id="0x00130A02" alias="transform_special">
        <toServer desc="特性宝石转换" class="com.kueem.webgame.game.action.game.GemAction" method="transform_special">
