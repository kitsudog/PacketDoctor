            <param fieldName="is_bind_first" tag="7" fieldType="bool" />
            <param fieldName="auto_buy" tag="8" fieldType="bool" />
        </toServer>
    </protocol>
    <protocol id="0x00130702" alias="batch_synthesis_result">
        <toClient desc="批量合成结果">
            <param tag="1" fieldType="int32" fieldName="" />
            <param tag="2" fieldType="int32" fieldName="" />
            <param tag="3" fieldType="int32" fieldName="" />
        </toClient>
    </protocol>
    <protocol id="0x00130802" alias="refine">
        <toServer desc="宝石提纯" class="com.kueem.webgame.game.action.game.GemAction" method="refine">
            <param fieldName="type" tag="1" fieldType="int32" />
            <param fieldName="index" tag="2" fieldType="int32" />
            <param fieldName="pos" tag="3" fieldType="int32" />
