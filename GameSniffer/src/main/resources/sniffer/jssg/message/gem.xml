            <param fieldName="auto_buy" tag="4" fieldType="bool" />
        </toServer>
    </protocol>
    <protocol id="0x00130402" alias="synthesis">
        <toServer desc="宝石合成" class="com.kueem.webgame.game.action.game.GemAction" method="synthesis">
            <param fieldName="indexList" tag="1" fieldType="int32" repeated="true" />
            <param fieldName="numList" tag="2" fieldType="int32" repeated="true" />
            <param fieldName="useBaohufu" tag="3" fieldType="bool" />
            <param fieldName="is_bind_first" tag="4" fieldType="bool" />
            <param fieldName="auto_buy" tag="5" fieldType="bool" />
        </toServer>
    </protocol>
    <protocol id="0x00130602" alias="batch_synthesis">
        <toServer desc="批量宝石合成" class="com.kueem.webgame.game.action.game.GemAction" method="batch_synthesis">
            <param fieldName="gemId" tag="1" fieldType="int32" />
            <param fieldName="bound" tag="2" fieldType="int32" />
            <param fieldName="gemNum" tag="3" fieldType="int32" />
            <param fieldName="num" tag="4" fieldType="int32" />
