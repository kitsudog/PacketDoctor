<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="quest">
	<protocol id="0x00110102" alias="syncInfo">
		<toClient desc="添加任务成功">
			<param fieldName="all" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="detail" tag="2" fieldType="int32" repeated="true" />
		</toClient>
		<toServer desc="同步任务数据"
			class="com.kueem.webgame.game.action.game.QuestAction" method="syncInfo">
		</toServer>
	</protocol>
	<protocol id="0x00110302" alias="accept">
		<toServer desc="完成任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="accept">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110402" alias="complete">
		<toServer desc="完成任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="complete">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110502" alias="cancle">
		<toServer desc="取消任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="cancle">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110602" alias="syncDayly">
		<toServer desc="同步日常任务信息"
			class="com.kueem.webgame.game.action.game.QuestAction" method="syncDayly">
			<param fieldName="typeList" tag="1" fieldType="int32" repeated="true" />
		</toServer>
	</protocol>

	<protocol id="0x00110702" alias="syncDaylyResult">
		<toClient desc="返回日常信息" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.QuestInfoProtoBuf.QuestInfoProto">
		</toClient>
	</protocol>

	<protocol id="0x00110802" alias="refreshForce">
		<toServer desc="刷新势力任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="refreshForce">
			<param fieldName="free" tag="1" fieldType="bool" />
			<param fieldName="autoBuy" tag="2" fieldType="bool" />
		</toServer>
	</protocol>

	<protocol id="0x00110902" alias="refreshEscort">
		<toServer desc="刷新护送任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="refreshEscort">
			<!-- 1 道具 2元宝(自动购买)3铜钱 -->
			<param fieldName="type" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110A02" alias="fastComplete">
		<toServer desc="快速完成"
			class="com.kueem.webgame.game.action.game.QuestAction" method="fastComplete">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110B02" alias="dialog">
		<toServer desc="任务对话"
			class="com.kueem.webgame.game.action.game.QuestAction" method="dialog">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00110C02" alias="refreshCampaign">
		<toServer desc="刷新讨伐任务数量"
			class="com.kueem.webgame.game.action.game.QuestAction" method="refreshCampaign">
		</toServer>
	</protocol>

	<protocol id="0x00110D02" alias="replyRefreshCampaign">
		<toClient desc="返回讨伐任务数量">
			<param fieldName="questNumbers" tag="1" fieldType="int32"
				repeated="true" desc="讨伐任务数量 " />
		</toClient>
	</protocol>

	<protocol id="0x00110E02" alias="releaseCampaign">
		<toServer desc="发布讨伐令"
			class="com.kueem.webgame.game.action.game.QuestAction" method="releaseCampaign">
			<param fieldName="type" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00111002" alias="acceptCampaignQuest">
		<toServer desc="接受讨伐令任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="acceptCampaignQuest">
			<param fieldName="type" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00111102" alias="fastCompleteAndCommit">
		<toServer desc="快速完成并提交任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="fastCompleteAndCommit">
			<param fieldName="qid" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00111202" alias="refreshCycle">
		<toServer desc="刷新循环任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="refreshCycle">
			<!-- 1 道具 2元宝(自动购买)3铜钱 4 免费 -->
			<param fieldName="type" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00111302" alias="fastCompleteBaizhan">
		<toServer desc="快速完成百战任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="fastCompleteBaizhan">
		</toServer>
	</protocol>

	<protocol id="0x00111402" alias="myXuanshangInfo">
		<toClient desc="返回我的悬赏任务排名信息">
			<param tag="1" fieldName="" fieldType="int32" desc="今日效率排名" />
			<param tag="2" fieldName="" fieldType="int32" desc="历史最高效率排名 " />
			<param tag="3" fieldName="" fieldType="int32" desc="本周积分排名" />
			<param tag="4" fieldName="" fieldType="int32" desc="历史积分排名 " />
			<param tag="5" fieldName="" fieldType="string" desc="今日效率 " />
			<param tag="6" fieldName="" fieldType="int32" desc="本周积分" />
			<param tag="7" fieldName="" fieldType="int32" desc="历史总积分 " />
		</toClient>
		<toServer desc="获取我的悬赏任务排名信息"
			class="com.kueem.webgame.game.action.game.QuestAction" method="myXuanshangInfo">
		</toServer>
	</protocol>
	<protocol id="0x00111502" alias="efficiencyRankings">
		<toClient desc="返回悬赏任务效率榜">
			<param tag="1" fieldName="" fieldType="int32" desc="排名"
				repeated="true" />
			<param tag="2" fieldName="" fieldType="string" desc="名字 "
				repeated="true" />
			<param tag="3" fieldName="" fieldType="int32" desc="等级"
				repeated="true" />
			<param tag="4" fieldName="" fieldType="int32" desc="职业id"
				repeated="true" />
			<param tag="5" fieldName="" fieldType="int32" desc="国家"
				repeated="true" />
			<param tag="6" fieldName="" fieldType="string" desc="完成时间"
				repeated="true" />
			<param tag="7" fieldName="type" fieldType="int32" desc="type" />
		</toClient>
		<toServer desc="获取悬赏任务效率榜"
			class="com.kueem.webgame.game.action.game.QuestAction" method="efficiencyRankings">
			<param tag="1" fieldName="type" fieldType="int32"
				desc="类型(1:今日排行榜;2:昨日排行榜;3:历史排行榜)" />
		</toServer>
	</protocol>
	<protocol id="0x00111602" alias="scoreRankings">
		<toClient desc="返回悬赏任务积分榜">
			<param tag="1" fieldName="" fieldType="int32" desc="排名"
				repeated="true" />
			<param tag="2" fieldName="" fieldType="string" desc="名字 "
				repeated="true" />
			<param tag="3" fieldName="" fieldType="int32" desc="等级"
				repeated="true" />
			<param tag="4" fieldName="" fieldType="int32" desc="职业id"
				repeated="true" />
			<param tag="5" fieldName="" fieldType="int32" desc="国家"
				repeated="true" />
			<param tag="6" fieldName="" fieldType="int32" desc="积分"
				repeated="true" />
			<param tag="7" fieldName="type" fieldType="int32" desc="type" />
		</toClient>
		<toServer desc="获取悬赏任务积分榜"
			class="com.kueem.webgame.game.action.game.QuestAction" method="scoreRankings">
			<param tag="1" fieldName="type" fieldType="int32"
				desc="类型(1:本周排行榜;2:上周排行榜;3:历史排行榜)" />
		</toServer>
	</protocol>

	<protocol id="0x00111702" alias="refreshFactionQuest">
		<toServer desc="刷新帮派任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="refreshFactionQuest">
		</toServer>
	</protocol>

	<protocol id="0x00111802" alias="helpRefresh">
		<toServer desc="帮刷"
			class="com.kueem.webgame.game.action.game.QuestAction" method="helpRefresh">
			<param tag="1" fieldName="username" fieldType="string" desc="玩家名字" />
		</toServer>
	</protocol>

	<protocol id="0x00111902" alias="syncFactionFortuneRank">
		<toClient desc="返回本帮派运势前五名的成员">
			<param tag="1" fieldName="name" fieldType="string" desc="名字"
				repeated="true" />
			<param tag="2" fieldName="fortune" fieldType="int32" desc="运势 "
				repeated="true" />
		</toClient>
		<toServer desc="获取本帮派运势前五名的成员"
			class="com.kueem.webgame.game.action.game.QuestAction" method="syncFactionFortuneRank">
		</toServer>
	</protocol>
	<protocol id="0x00111A02" alias="syncFactionMemberQuest">
		<toClient desc="同步本帮派成员的帮派任务情况">
			<param tag="1" fieldName="name" fieldType="string" desc="名字"
				repeated="true" />
			<param tag="2" fieldName="fortune" fieldType="int32" desc="运势 "
				repeated="true" />
			<param tag="3" fieldName="questId" fieldType="int32" desc="任务id"
				repeated="true" />
			<param tag="4" fieldName="state" fieldType="int32"
				desc="任务状态(0:未接;1:已接;2:完成)" repeated="true" />
			<param tag="5" fieldName="times" fieldType="int32" desc="被刷次数"
				repeated="true" />
			<param tag="6" fieldName="position" fieldType="int32" desc="帮派职位"
				repeated="true" />
			<param tag="7" fieldName="online" fieldType="bool" desc="是否在线"
				repeated="true" />
		</toClient>
		<toServer desc="获取本帮派成员的帮派任务情况"
			class="com.kueem.webgame.game.action.game.QuestAction" method="syncFactionMemberQuest">
		</toServer>
	</protocol>
	<protocol id="0x00111B02" alias="syncFactionRefreshedRecord">
		<toClient desc="同步帮派任务被刷新日志">
			<param tag="1" fieldName="name" fieldType="string" desc="名字"
				repeated="true" />
			<param tag="2" fieldName="fortune" fieldType="int32" desc="运势"
				repeated="true" />
		</toClient>
		<toServer desc="获取帮派任务被刷新日志"
			class="com.kueem.webgame.game.action.game.QuestAction" method="syncFactionRefreshedRecord">
		</toServer>
	</protocol>

	<protocol id="0x00111C02" alias="tryCompleteTask">
		<toServer desc="尝试完成任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="tryCompleteTask">
			<param tag="1" fieldName="qid" fieldType="int32" desc="任务id" />
			<param tag="2" fieldName="param" fieldType="int32" desc="参数" />
		</toServer>
	</protocol>


	<protocol id="0x00112002" alias="requestFuckZhaoYunInfo">
		<toClient desc="同步单骑救主任务详情">
			<param tag="1" fieldName="shakeTime" fieldType="int32" desc="掷骰次数" />
			<param tag="2" fieldName="freeTime" fieldType="int32" desc="剩余掷骰次数" />
			<param tag="3" fieldName="buyTime" fieldType="int32" desc="已经购买的次数" />
			<param tag="4" fieldName="location" fieldType="int32" desc="现在的位置" />
			<param tag="5" fieldName="questId" fieldType="int32" desc="身上任务的id" />
			<param tag="6" fieldName="state" fieldType="int32"
				desc="现在的状态0：可以前进，1：任务未完成" />
			<param tag="7" fieldName="state" fieldType="int32" repeated="true"
				desc="四个箱子的状态0:灰,1:亮,2:开[0,0,0,0]" />
			<param tag="8" fieldName="buyTime" fieldType="int32" desc="本周我的圈数" />
			<param tag="9" fieldName="location" fieldType="int32" desc="本周我的排名" />
			<param tag="10" fieldName="questId" fieldType="int32" repeated="true"
				desc="数组三个卡片剩余的次数" />
		</toClient>
		<toServer desc="获取帮派任务被刷新日志"
			class="com.kueem.webgame.game.action.game.QuestAction" method="requestFuckZhaoYunInfo">
		</toServer>
	</protocol>

	<protocol id="0x00112102" alias="shakeZhaoYun">
		<toClient desc="同步掷骰点数">
			<param tag="1" fieldName="num" fieldType="int32" desc="掷骰点数" />
			<param tag="2" fieldName="useTool" fieldType="int32" desc="使用双倍点数道具 0、1" />
		</toClient>
		<toServer desc="掷骰子"
			class="com.kueem.webgame.game.action.game.QuestAction" method="shakeZhaoYun">
			<param tag="1" fieldName="card1" fieldType="int32" desc="0不使用/1使用:遥控卡 " />
			<param tag="2" fieldName="card2" fieldType="int32" desc="0不使用/1使用:幸运卡" />
			<param tag="3" fieldName="card3" fieldType="int32" desc="0不使用/1使用 :加速卡" />
		</toServer>
	</protocol>

	<protocol id="0x00112202" alias="buyShakeTime">
		<toServer desc="买骰子"
			class="com.kueem.webgame.game.action.game.QuestAction" method="buyShakeTime">
		</toServer>
	</protocol>

	<protocol id="0x00112302" alias="openBox">
		<toServer desc="打开盒子"
			class="com.kueem.webgame.game.action.game.QuestAction" method="openBox">
			<param tag="1" fieldName="type" fieldType="int32" desc="0123:[0,0,0,0]" />
		</toServer>
	</protocol>

	<protocol id="0x00112402" alias="giveUpAcceptQuest">
		<toServer desc="不接受任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="giveUpAcceptQuest">
		</toServer>
	</protocol>

	<protocol id="0x00112502" alias="costCoinfastCompleteQuest">
		<toServer desc="花费元宝 快速完成任务"
			class="com.kueem.webgame.game.action.game.QuestAction" method="costCoinfastCompleteQuest">
			<param tag="1" fieldName="questId" fieldType="int32" desc="任务id" />
		</toServer>
	</protocol>




</config>
