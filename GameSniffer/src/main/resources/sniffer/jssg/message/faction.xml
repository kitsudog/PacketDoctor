<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="faction">
    <!-- 帮派系统模块 0x16 -->
    <protocol id="0x00160102" alias="gainFactions">
        <toClient desc="返回帮派列表" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto">
        </toClient>
        <toServer desc="获取帮派列表"
         class="com.kueem.webgame.game.action.game.FactionAction" method="gainFactions">
            <param fieldName="page" tag="1" fieldType="int32"/>
            <param fieldName="camp" tag="2" fieldType="int32"/>
            <param fieldName="type" tag="3" fieldType="int32"/><!-- 搜索类型. 0:全部;
            1:按帮派名字搜索; 2:按帮主名字搜索; -->
            <param fieldName="name" tag="4" fieldType="string"/>
            <param fieldName="onlyNotFull" tag="5" fieldType="bool"/>
        </toServer>
    </protocol>
    <protocol id="0x00160202" alias="info">
        <toServer desc="获取当前玩家帮派信息"
         class="com.kueem.webgame.game.action.game.FactionAction" method="info">
        </toServer>
    </protocol>
    <protocol id="0x00160302" alias="infoResult">
        <toClient desc="同步当前玩家帮派信息" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto.Faction">
            <param tag="1" fieldType="int32" fieldName=""/>
        </toClient>
    </protocol>
    <protocol id="0x00160402" alias="view">
        <toClient desc="返回帮派信息" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto.Faction">
        </toClient>
        <toServer desc="查看帮派信息"
         class="com.kueem.webgame.game.action.game.FactionAction" method="view">
            <param fieldName="faction_name" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00160502" alias="createFaction">
        <toClient desc="告诉客户端已经创建成功">
            <param fieldName="success" tag="1" fieldType="bool" desc="是否创建成功 true 成功 fsle 失败"/>
        </toClient>
        <toServer desc="创建帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="createFaction">
            <param fieldName="type" tag="1" fieldType="int32"/><!-- 创建类型(1:花铜钱;2:使用建帮令) -->
            <param fieldName="name" tag="2" fieldType="string"/>
            <param fieldName="aim" tag="3" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00160602" alias="applyJoinIn">
        <toServer desc="申请加入帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="applyJoinIn">
            <param fieldName="faction_id" tag="1" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00160702" alias="callOffApply">
        <toServer desc="取消申请"
         class="com.kueem.webgame.game.action.game.FactionAction" method="callOffApply">
            <param fieldName="faction_id" tag="1" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00160802" alias="applyListSync">
        <toClient desc="同步申请者列表" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto.Faction">
        </toClient>
    </protocol>
    <protocol id="0x00160A02" alias="agreeToJoinIn">
        <toServer desc="帮派领导同意玩家加入本帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="agreeToJoinIn">
            <!--param fieldName="faction_id" tag="1" fieldType="int32"/ -->
            <param fieldName="user_id" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00160B02" alias="rejectJoinIn">
        <toServer desc="帮派领导拒绝玩家加入本帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="rejectJoinIn">
            <!--param fieldName="faction_id" tag="1" fieldType="int32"/ -->
            <param fieldName="user_id" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00160C02" alias="gainMemberList">
        <toServer desc="获取成员列表"
         class="com.kueem.webgame.game.action.game.FactionAction" method="gainMemberList">
        </toServer>
    </protocol>
    <protocol id="0x00160D02" alias="gainMemberListResult">
        <toClient desc="更新帮派成员信息" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto.Faction">
        </toClient>
    </protocol>
    <protocol id="0x00160E02" alias="appoint">
        <toServer desc="任命"
         class="com.kueem.webgame.game.action.game.FactionAction" method="appoint">
            <param fieldName="user_id" tag="1" fieldType="string"/>
            <param fieldName="position" tag="2" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00160F02" alias="kickout">
        <toServer desc="踢出"
         class="com.kueem.webgame.game.action.game.FactionAction" method="kickout">
            <param fieldName="user_id" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00161102" alias="modifyInfo">
        <toServer desc="修改资料(QQ,YY,宗旨)"
         class="com.kueem.webgame.game.action.game.FactionAction" method="modifyInfo">
            <param fieldName="type" tag="1" fieldType="int32"/><!--1代表QQ, 2代表YY,
            3代表宗旨 -->
            <param fieldName="content" tag="2" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00161202" alias="notice">
        <toServer desc="群发公告"
         class="com.kueem.webgame.game.action.game.FactionAction" method="notice">
            <param fieldName="type" tag="1" fieldType="int32"/><!--1代表发送窗口,
            2代表发送邮件 -->
            <param fieldName="title" tag="2" fieldType="string"/>
            <param fieldName="content" tag="3" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00161302" alias="dismiss">
        <toServer desc="解散帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="dismiss">
        </toServer>
    </protocol>
    <protocol id="0x00161402" alias="learnSkill">
        <toServer desc="学习技能"
         class="com.kueem.webgame.game.action.game.FactionAction" method="learnSkill">
            <param fieldName="skillId" tag="1" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00161502" alias="upgrade_building">
        <toServer desc="升级建筑"
         class="com.kueem.webgame.game.action.game.FactionAction" method="upgrade_building">
            <param fieldName="building_id" tag="1" fieldType="int32"/>
            <param fieldName="type" tag="2" fieldType="int32"
             desc="type=1:开始升级;type=2:结束升级"/>
        </toServer>
    </protocol>
    <protocol id="0x00161602" alias="invite">
        <toServer desc="邀请加入帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="invite">
            <param fieldName="user_id" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    <protocol id="0x00161702" alias="gainInviteList">
        <toServer desc="获得当前用户的邀请列表"
         class="com.kueem.webgame.game.action.game.FactionAction" method="gainInviteList">
        </toServer>
    </protocol>
    <protocol id="0x00161802" alias="gainInviteListResult">
        <toClient desc="同步邀请者列表到被邀请的玩家" proto="true"
         messageClass="com.kueem.webgame.game.protobuf.FactionBuffers.FactionsProto">
        </toClient>
    </protocol>
    <protocol id="0x00161902" alias="receive">
        <toServer desc="接受邀请"
         class="com.kueem.webgame.game.action.game.FactionAction" method="receive">
            <param fieldName="faction_id" tag="1" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00161A02" alias="reject">
        <toServer desc="拒绝邀请"
         class="com.kueem.webgame.game.action.game.FactionAction" method="reject">
            <param fieldName="faction_id" tag="1" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00161B02" alias="exit">
        <toServer desc="退出帮派"
         class="com.kueem.webgame.game.action.game.FactionAction" method="exit">
        </toServer>
    </protocol>
    <protocol id="0x00161C02" alias="impeach">
        <toServer desc="弹劾帮主"
         class="com.kueem.webgame.game.action.game.FactionAction" method="impeach">
        </toServer>
    </protocol>
    <protocol id="0x00161D02" alias="getSkillInfo">
        <toServer desc="获取帮派技能"
         class="com.kueem.webgame.game.action.game.FactionAction" method="getSkillInfo">
        </toServer>
    </protocol>
    <protocol id="0x00161E02" alias="skillInfoResult">
        <toClient desc="同步帮派技能信息">
            <param fieldName="num" tag="1" fieldType="int32"/><!--今日已学习次数 -->
            <param fieldName="ids" tag="2" fieldType="int32" repeated="true"/><!--技能id列表 -->
            <param fieldName="levels" tag="3" fieldType="int32" repeated="true"/><!--技能等级列表 -->
        </toClient>
    </protocol>
    <protocol id="0x00161F02" alias="contribute">
        <toServer desc="捐献"
         class="com.kueem.webgame.game.action.game.FactionAction" method="contribute">
            <param fieldName="type" tag="1" fieldType="int32"/><!--1铜钱2军令3元宝 -->
            <param fieldName="num" tag="2" fieldType="int32"/>
        </toServer>
    </protocol>
    <protocol id="0x00162002" alias="welfare">
        <toServer desc="领取福利"
         class="com.kueem.webgame.game.action.game.FactionAction" method="welfare">
        </toServer>
    </protocol>
    <protocol id="0x00162102" alias="refute">
        <toServer desc="帮主反驳弹劾"
         class="com.kueem.webgame.game.action.game.FactionAction" method="refute">
        </toServer>
    </protocol>
    <protocol id="0x00162202" alias="impeachInfo">
        <toClient desc="同步帮派弹劾信息">
            <param fieldName="master" tag="1" fieldType="string"/><!-- 帮主名字 -->
            <param fieldName="candidate" tag="2" fieldType="string"/><!-- 候选人的名字 -->
            <param fieldName="type" tag="3" fieldType="int32"/><!-- 弹劾类型 -->
            <param fieldName="endTime" tag="4" fieldType="int64"/><!-- 弹劾期结束时间 -->
        </toClient>
    </protocol>
    <protocol id="0x00162302" alias="canImpeach">
        <toClient desc="返回结果">
            <param fieldName="result" tag="1" fieldType="bool"/>
        </toClient>
        <toServer desc="预判断可否弹劾"
         class="com.kueem.webgame.game.action.game.FactionAction" method="canImpeach">
        </toServer>
    </protocol>
    
    <protocol id="0x00162402" alias="fudai">
        <toClient desc="返回结果">
            <param fieldName="state" tag="1" fieldType="int32" repeated="true"/><!--6个状态
            : 0: 不可领取,1:可以领取,2:已经领取过 -->
            <param fieldName="getBindReward" tag="2" fieldType="int32"/><!--消耗30贡献可以领取一次奖励
            :0:未领取,非0 :已领取 -->
            <param fieldName="dailyCon" tag="3" fieldType="int32"/><!-- 当日贡献 -->
            <param fieldName="totalCon" tag="4" fieldType="int32"/><!-- 总贡献 -->
        </toClient>
        <toServer desc="请求福袋信息"
         class="com.kueem.webgame.game.action.game.FactionAction" method="fudai">
        </toServer>
    </protocol>
    
    <protocol id="0x00162502" alias="getFactionGift">
        <toServer desc="领取福袋"
         class="com.kueem.webgame.game.action.game.FactionAction" method="getFactionGift">
            <param fieldName="type" tag="1" fieldType="int32"/><!--福袋index -->
        </toServer>
    </protocol>
    
    <protocol id="0x00162602" alias="getBindCash">
        <toClient desc="返回结果">
            <param fieldName="itemId" tag="1" fieldType="int32"/><!--0:未领取,非0:itemId -->
        </toClient>
        <toServer desc="领取消耗30贡献的奖励 "
         class="com.kueem.webgame.game.action.game.FactionAction" method="getBindCash">
        </toServer>
    </protocol>
    
    <protocol id="0x00162702" alias="applyMerge">
        <toServer desc="申请合并 "
         class="com.kueem.webgame.game.action.game.FactionAction" method="applyMerge">
            <param desc="目标帮派 名字" fieldName="faction_name" tag="1" fieldType="string"/>
        </toServer>
    </protocol>
    
    <protocol id="0x00162802" alias="notifyInvited">
        <toClient desc="通知玩家，有人邀请加入帮派">
            <param fieldName="factionName" tag="1" fieldType="string" desc="帮派名字"/>
            <param fieldName="factionId" tag="2" fieldType="int32" desc="帮派id"/>
        </toClient>
    </protocol>
    
    <protocol id="0x00162902" alias="notifyApplied">
        <toClient desc="通知领导，有人申请加入帮派">
            <param fieldName="userId" tag="1" fieldType="string" desc="用户id"/>
        </toClient>
    </protocol>
    
    
    
    <protocol id="0x00162A02" alias="syncFactionBuyInfo">
        <toClient desc="同步帮派商城帮派购买情况">
            <param fieldName="id" tag="1" fieldType="int32" repeated="true"
             desc="对应faction_shop表id"/>
            <param fieldName="num" tag="2" fieldType="int32" repeated="true"
             desc="购买个数"/>
        </toClient>
        <toServer desc="获取帮派商城帮派购买情况"
         class="com.kueem.webgame.game.action.game.FactionAction" method="syncFactionBuyInfo">
        </toServer>
    </protocol>
    
    <protocol id="0x00162B02" alias="syncPersonalBuyInfo">
        <toClient desc="同步帮派商城个人购买情况">
            <param fieldName="id" tag="1" fieldType="int32" repeated="true"
             desc="对应faction_shop表id"/>
            <param fieldName="num" tag="2" fieldType="int32" repeated="true"
             desc="购买个数"/>
        </toClient>
        <toServer desc="获取帮派商城个人购买情况"
         class="com.kueem.webgame.game.action.game.FactionAction" method="syncPersonalBuyInfo">
        </toServer>
    </protocol>
    
    <protocol id="0x00162C02" alias="buy">
        <toServer desc="购买"
         class="com.kueem.webgame.game.action.game.FactionAction" method="buy">
            <param fieldName="faction_shop_goods_id" tag="1" fieldType="int32"
             desc="对应faction_shop表id"/>
            <param fieldName="num" tag="2" fieldType="int32"/>
        </toServer>
    </protocol>
    
    <protocol id="0x00162D02" alias="syncWareHouseDate">
        <toClient desc="同步帮派仓库物品">
            <param fieldName="itemId" tag="1" fieldType="int32" repeated="true"
             desc="对应itemId表id"/>
            <param fieldName="type" tag="2" fieldType="int32" repeated="true"
             desc="对应itemId 的类型 "/>
            <param fieldName="num" tag="3" fieldType="int32" repeated="true"
             desc="数量"/>
        </toClient>
        
        <toServer desc="请求同步帮派仓库物品"
         class="com.kueem.webgame.game.action.game.FactionAction" method="syncWareHouseDate">
        </toServer>
    </protocol>
    
    <protocol id="0x00162F02" alias="shareItem">
        <toServer desc="把物品分配给用户 最多3个"
         class="com.kueem.webgame.game.action.game.FactionAction" method="shareItem">
            <param fieldName="index1" tag="1" fieldType="int32"/><!-- 格子的索引 -->
            <param fieldName="num1" tag="2" fieldType="int32"/><!-- 数量 -->
            <param fieldName="index2" tag="3" fieldType="int32"/>
            <param fieldName="num2" tag="4" fieldType="int32"/>
            <param fieldName="index3" tag="5" fieldType="int32"/>
            <param fieldName="num3" tag="6" fieldType="int32"/>
            <param fieldName="user_id" tag="7" fieldType="string"/>
        </toServer>
    </protocol>
    
    <protocol id="0x00163002" alias="addCashCowLingLi">
        <toServer desc="给摇钱树加灵力"
         class="com.kueem.webgame.game.action.game.FactionAction" method="addCashCowLingLi">
        </toServer>
    </protocol>
    
    <protocol id="0x00163102" alias="rockCashCow">
        <toServer desc="摇动摇钱树"
         class="com.kueem.webgame.game.action.game.FactionAction" method="rockCashCow">
        </toServer>
    </protocol>
    
    <protocol id="0x00163202" alias="requestFactionRecord">
        <toClient desc="返回结果">
            <param fieldName="page" tag="1" fieldType="int32"/>
            <param fieldName="total_page" tag="2" fieldType="int32"/>
            <param fieldName="type" tag="3" fieldType="int32"/>
            <param fieldName="message" tag="4" fieldType="string" repeated="true"/>
            <param fieldName="time" tag="5" fieldType="string" repeated="true"/>
        </toClient>
        <toServer desc="请求帮派成员活动信息 "
         class="com.kueem.webgame.game.action.game.FactionAction" method="requestFactionRecord">
            <param fieldName="type" tag="1" fieldType="int32"/>
            <param fieldName="page" tag="2" fieldType="int32"/>
        </toServer>
    </protocol>
    
    
    <protocol id="0x00164002" alias="jisi">
        <toServer desc="祭祀"
         class="com.kueem.webgame.game.action.game.FactionAction" method="jisi">
        </toServer>
    </protocol>
    
    <protocol id="0x00164102" alias="zhaohuan">
        <toServer desc="召唤神兽" class="com.kueem.webgame.game.action.game.FactionAction" method="zhaohuan">
        </toServer>
    </protocol>
    
    <protocol id="0x00164202" alias="notifyAnimalMapOpen">
        <toClient desc="通知帮派成员神兽地图开启">
            <param fieldName="userName" tag="1" fieldType="string" desc="开启者姓名"/>
            <param fieldName="position" tag="2" fieldType="int32" desc="开启者职位"/>
            <param fieldName="showUpTime" tag="3" fieldType="int64" desc="神兽出现时间"/>
        </toClient>
    </protocol>
    
    <protocol id="0x00164302" alias="syncAnimalMapDamageAndXp">
        <toClient desc="同步在神兽副本中的累积伤害和累积经验">
            <param tag="1" fieldName="damage" fieldType="int32" desc="本次的伤害值"/>
            <param tag="2" fieldName="xp" fieldType="int32" desc="本次的经验值"/>
        </toClient>
    </protocol>
    
    <protocol id="0x00164402" alias="gainAnimalMapReward">
        <toServer desc="击杀神兽后获取奖励" class="com.kueem.webgame.game.action.game.FactionAction" method="gainAnimalMapReward">
        </toServer>
    </protocol>
    
    <!-- 帮派宴会相关 50开头 请勿占用 -->

    <protocol id="0x00165002" alias="factionPartyInfo">
        <!-- 成功后同步 0x00160302 协议 -->
        <toServer desc="获取帮派宴会开启时间和类型"
            class="com.kueem.webgame.game.action.game.FactionAction" method="factionPartyInfo">
        </toServer>
    </protocol>

    <protocol id="0x00165102" alias="hostParty">
        <!-- 成功后同步 0x00160302协议 -->
        <toServer desc="举办宴会"
            class="com.kueem.webgame.game.action.game.FactionAction" method="hostParty">
            <param fieldName="partyType" tag="1" fieldType="int32" desc="宴会类型 1 2 3" />
            <param fieldName="partyTime" tag="2" fieldType="int32" desc="举办时间1 到 12点的整数" />
        </toServer>
    </protocol>

    <protocol id="0x00165202" alias="upgradeParty">
        <!-- 成功后同步 0x00160302协议 -->
        <toServer desc="升级宴会宴会"
            class="com.kueem.webgame.game.action.game.FactionAction" method="upgradeParty">
            <param fieldName="partyType" tag="1" fieldType="int32" desc="宴会类型 1 2 3" />
        </toServer>
    </protocol>

    <protocol id="0x00165302" alias="firework">

        <toClient desc="通知客户端有人燃放烟花">
            <param tag="1" fieldType="string" fieldName="uid" desc="燃放烟花的玩家的ID" />
        </toClient>

        <!-- 成功后通知客户端有人燃放烟花，同时同步0x00165502协议 -->

        <toServer desc="燃放烟花"
            class="com.kueem.webgame.game.action.game.FactionAction" method="firework">
        </toServer>

    </protocol>

    <protocol id="0x00165402" alias="callmaid">

        <toClient desc="通知客户端有人召唤女仆">
            <param tag="1" fieldType="string" fieldName="uid" desc="召唤女仆的玩家的ID" />
        </toClient>

        <!-- 成功后通知客户端有人召唤女仆，同时同步0x00165502协议 -->
        <toServer desc="召唤女仆"
            class="com.kueem.webgame.game.action.game.FactionAction" method="callmaid">
        </toServer>

    </protocol>

    <protocol id="0x00165502" alias="partyInfo">
        <toClient desc="宴会全局信息">
            <param fieldName="playerCount" tag="1" fieldType="int32" desc="宴会参与人数" />
            <param fieldName="hot" tag="2" fieldType="int32" desc="宴会气氛值" />
        </toClient>
    </protocol>

    <protocol id="0x00165602" alias="partySelfInfo">
        <toClient desc="宴会自身信息">
            <param fieldName="xp" tag="1" fieldType="int32" desc="获取到的经验" />
            <param fieldName="aura" tag="2" fieldType="int32" desc="获取到的灵气" />
        </toClient>
    </protocol>
    
</config>
