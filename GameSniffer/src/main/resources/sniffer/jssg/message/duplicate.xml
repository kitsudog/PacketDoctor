<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="duplicate">

	<protocol id="0x00140702" alias="accumulate">
		<toClient desc="累积经验">
			<param fieldName="accumulateXp" tag="1" fieldType="int64"
				repeated="false" />
		</toClient>
	</protocol>
	<protocol id="0x00140802" alias="syncDropSkill">
		<toClient desc="同步掉落技能 ">
			<param fieldName="skillId" tag="1" fieldType="int32" repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x00140902" alias="rebirth">
		<toClient desc="弹出复活面板">
			<param fieldName="x" tag="1" fieldType="int32" desc="战士的位置X" />
			<param fieldName="y" tag="2" fieldType="int32" desc="战士的位置Y" />
			<param fieldName="attackerName" tag="3" fieldType="string" />
			<param fieldName="attackerLevel" tag="4" fieldType="int32" />
			<param fieldName="attackerCamp" tag="5" fieldType="int32" />
			<param fieldName="perfectRebirthCost" tag="6" fieldType="int32" />
			<param fieldName="dieTime" tag="7" fieldType="int64" />

		</toClient>
		<toServer desc="复活"
			class="com.kueem.webgame.game.action.game.SceneAction" method="rebirth">
			<param fieldName="rebirthType" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00140A02" alias="finish">
		<toClient desc="副本成功或失败">
			<param fieldName="status" tag="1" fieldType="int32" desc="副本状态 1表示成功  2表示失败" />
			<param fieldName="userTime" tag="2" fieldType="int32" desc="完成副本时间  (单位秒)" />
			<param fieldName="killNum" tag="3" fieldType="int32" desc="击杀怪物" />
			<param fieldName="gainXp" tag="4" fieldType="int32" desc="获得总经验" />
			<param fieldName="maxCombos" tag="5" fieldType="int32" desc="连击数" />
			<param fieldName="grade" tag="6" fieldType="int32" desc="通关评分" />
			<param fieldName="hasWard" tag="7" fieldType="int32" desc="是否有奖励领取" />
		</toClient>
	</protocol>
	<protocol id="0x00140B02" alias="lottery">
		<toServer desc="翻牌"
			class="com.kueem.webgame.game.action.game.SceneAction" method="lottery">
			<param fieldName="pos" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00140C02" alias="lotteryResult">
		<toClient desc="翻牌返回结果" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.LotteryBuffers.LotteryProto">
		</toClient>
	</protocol>
	<protocol id="0x00140D02" alias="syncMonsterNum">
		<toClient desc="同步当前区域剩余怪物数量和总数量 (计算数量时不包括地物)">
			<param fieldName="currentNum" tag="1" fieldType="int32" />
			<param fieldName="allNum" tag="2" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00140E02" alias="finishStep">
		<toClient desc="副本完成后的步骤">
			<param fieldName="step" tag="1" fieldType="int32"
				desc="步骤1.播放死亡动画2.拾取掉落宝箱、奖励物品" />
			<param fieldName="stepTime" tag="2" fieldType="int32" desc="当前步骤时间" />
		</toClient>
	</protocol>
	<protocol id="0x00140F02" alias="mosterEnterArea">
		<toClient desc="怪物进去区域视野">
			<param fieldName="areaId" tag="1" fieldType="int32" />
			<param fieldName="countDownTime" tag="2" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00141002" alias="syncCountXp">
		<toClient desc="同步爬塔类型累积经验奖励">
			<param fieldName="countXp" tag="1" fieldType="int64" repeated="false" />
		</toClient>
	</protocol>
	<protocol id="0x00141102" alias="syncRemainTime">
		<toClient desc="同步爬塔类型剩余时间">
			<param fieldName="remainTime" tag="1" fieldType="int32"
				repeated="false" />
		</toClient>
	</protocol>
	<protocol id="0x00141202" alias="finishGate">
		<toClient desc="完成当前关卡">
		</toClient>
	</protocol>
	<protocol id="0x00141302" alias="syncCountDownTime">
		<toClient desc="同步倒计时状态">
			<param fieldName="status" tag="1" fieldType="int32" />
			<param fieldName="remainTime" tag="2" fieldType="string" />
		</toClient>
	</protocol>

	<protocol id="0x00141402" alias="getScoreboard">
		<toServer desc="竞技场：获取积分榜的信息"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="scoreboard">
			<param fieldName="page" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00141502" alias="returnScoreboard">
		<toClient desc="竞技场：返回积分榜的信息">
			<param fieldName="rankings" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="name" tag="2" fieldType="string" repeated="true" />
			<param fieldName="level" tag="3" fieldType="int32" repeated="true" />
			<param fieldName="job" tag="4" fieldType="int32" repeated="true" />
			<param fieldName="score" tag="5" fieldType="int32" repeated="true" />
			<param fieldName="winRate" tag="6" fieldType="int32" repeated="true" />
			<param fieldName="force" tag="7" fieldType="int32" repeated="true" />
			<param fieldName="winTimes" tag="8" fieldType="int32" repeated="true" />
			<param fieldName="currentPage" tag="9" fieldType="int32" />
			<param fieldName="totalPage" tag="10" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00141602" alias="arenaInfo">
		<toServer desc="竞技场：获取当前玩家的竞技场信息"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="arenaInfo">
		</toServer>
	</protocol>
	<protocol id="0x00141702" alias="returnArenaInfo">
		<toClient desc="竞技场：返回当前玩家的竞技场信息">
			<param fieldName="score" tag="1" fieldType="int32" />
			<param fieldName="rankings" tag="2" fieldType="int32" />
			<param fieldName="winTime" tag="3" fieldType="int32" />
			<param fieldName="loseTime" tag="4" fieldType="int32" />
			<param fieldName="winRate" tag="5" fieldType="int32" />
			<param fieldName="todayTime" tag="6" fieldType="int32" />
			<param fieldName="matchingNum" tag="7" fieldType="int32" />
			<param fieldName="coupons_lastWeek" tag="8" fieldType="int32" />
			<param fieldName="week" tag="9" fieldType="int32" />
			<param fieldName="highest" tag="10" fieldType="int32" />
		</toClient>
	</protocol>
	<protocol id="0x00141802" alias="gameList">
		<toServer desc="竞技场：获取比赛列表"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="gameList">
		</toServer>
	</protocol>
	<protocol id="0x00141902" alias="returnGameList">
		<toClient desc="竞技场：返回比赛列表">
			<param fieldName="gameId" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="name_left" tag="2" fieldType="string"
				repeated="true" />
			<param fieldName="level_left" tag="3" fieldType="int32"
				repeated="true" />
			<param fieldName="job_left" tag="4" fieldType="int32" repeated="true" />
			<param fieldName="name_right" tag="5" fieldType="string"
				repeated="true" />
			<param fieldName="level_right" tag="6" fieldType="int32"
				repeated="true" />
			<param fieldName="job_right" tag="7" fieldType="int32"
				repeated="true" />
		</toClient>
	</protocol>
	<protocol id="0x00141A02" alias="watchingGame">
		<toServer desc="竞技场：观战"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="watchGame">
			<param fieldName="gameId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
	<protocol id="0x00141B02" alias="match">
		<toServer desc="竞技场：报名,开始匹配"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="match">
			<param fieldName="type" tag="1" fieldType="int32" /><!--0:单人模式; 
				1:多人模式 -->
		</toServer>
	</protocol>
	<protocol id="0x00141C02" alias="matchResult">
		<toClient desc="竞技场：通知客户端,匹配结果">
			<param fieldName="type" tag="1" fieldType="int32" /><!--0:单人模式; 
				1:多人模式 -->
			<param fieldName="type" tag="2" fieldType="bool" /><!--true:成功; 
				false:失败 -->
		</toClient>
	</protocol>
	<protocol id="0x00141D02" alias="startGame">
		<toServer desc="竞技场：开始竞技"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="startGame">
		</toServer>
	</protocol>

	<protocol id="0x00141E02" alias="showPvpStatus">
		<toClient desc="单人竞技场，结果显示" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.ArenaResult.ArenaResultProto">
		</toClient>
	</protocol>
	<protocol id="0x00141F02" alias="prepareStep">
		<toClient desc="初始化副本，准备步骤">
			<param fieldName="step" tag="1" fieldType="int32" />
			<param fieldName="stepTime" tag="2" fieldType="int32" desc="当前步骤时间" />
		</toClient>
	</protocol>
	<protocol id="0x00142002" alias="cancelMatch">
		<toServer desc="竞技场：取消匹配"
			class="com.kueem.webgame.game.action.game.ArenaAction" method="cancelMatch">
		</toServer>
	</protocol>


	<protocol id="0x00145002" alias="getBossLefttime">
		<toClient desc="返回">
			<param fieldName="id" tag="1" fieldType="int32" repeated="true"
				desc="世界Boss 主键id" />
			<param fieldName="remainTime" tag="2" fieldType="int32"
				repeated="true" desc="当前步骤时间" />
		</toClient>
		<toServer desc="请求世界boss剩余时间"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="getBossLefttime">
		</toServer>
	</protocol>
	<protocol id="0x00145102" alias="syncWorldBossTeamInfo">
		<toClient desc="同步世界Boss小地图，队友信息">
			<param fieldName="mapId" tag="1" fieldType="int32" repeated="true"
				desc="队友所在地图id" />
			<param fieldName="uid" tag="2" fieldType="string" repeated="true"
				desc="队友id" />
			<param fieldName="name" tag="3" fieldType="string" repeated="true"
				desc="队友名称" />
		</toClient>
	</protocol>

	<protocol id="0x00146002" alias="organ_msg">
		<toClient desc="机关触发提示">
			<param fieldName="organ_id" tag="1" fieldType="int32" />
		</toClient>
	</protocol>

	<protocol id="0x00146102" alias="faction_battle_info">
		<toClient desc="同步帮派实时统计信息">
			<param fieldName="factionScore" tag="1" fieldType="int32" desc="帮派战分" />
			<param fieldName="personalScore" tag="2" fieldType="int32"
				desc="个人战功" />
			<param fieldName="rebirthLeft" tag="3" fieldType="int32" desc="剩余复活次数" />
			<param fieldName="selfFactionName" tag="4" fieldType="string"
				desc="己方帮派名称" />
			<param fieldName="armyFactionName" tag="5" fieldType="string"
				desc="敌方帮派名称" />
		</toClient>
	</protocol>


	<protocol id="0x00146202" alias="buyJunling">
		<toServer desc="购买军令"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="buyJunling">
		</toServer>
	</protocol>

	<protocol id="0x00146302" alias="wipeOut">
		<toClient desc="扫荡返回">
			<param fieldName="ret" tag="1" fieldType="bool" desc="是否成功开始扫荡" />
			<param fieldName="ret" tag="2" fieldType="int32" desc="时间" />
		</toClient>
		<toServer desc="开始扫荡"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="wipeOut">
			<param fieldName="mapId" tag="1" fieldType="int32" desc="地图ID" />
			<param fieldName="count" tag="2" fieldType="int32" desc="扫荡次数" />
		</toServer>
	</protocol>

	<protocol id="0x00146402" alias="wipeOutRet">
		<toClient desc="单次扫荡的结果">
			<param fieldName="xp" tag="1" fieldType="int32" desc="本次扫荡获取的经验" />
			<param fieldName="rewardId" tag="2" fieldType="int32" repeated="true"
				desc="本次扫荡获取的道具的rewardID" />
			<param fieldName="count" tag="3" fieldType="int32" desc="第几次" />
		</toClient>
	</protocol>

	<protocol id="0x00146502" alias="speedWipeOut">
		<toServer desc="加速扫荡"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="speedWipeOut">
		</toServer>
	</protocol>

	<protocol id="0x00146602" alias="cancleWipeOut">
		<toClient desc="取消扫荡的结果">
			<param fieldName="reason" tag="1" fieldType="int32"
				desc="取消扫荡的结果 0 正常取消 1 背包满了 2军令不足" />
		</toClient>
		<toServer desc="取消扫荡"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="cancleWipeOut">
		</toServer>
	</protocol>

	<protocol id="0x00146702" alias="fetchJunlingInfo">
		<toClient desc="返回请求获取军令相关的信息">
			<param fieldName="jungling" tag="1" fieldType="int32" desc="军令数量" />
			<param fieldName="jungling_buy_count" tag="2" fieldType="int32"
				desc="购买次数" />
		</toClient>
		<toServer desc="请求获取军令相关的信息"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="fetchJunlingInfo">
		</toServer>
	</protocol>

	<protocol id="0x00146802" alias="syncCampBossMapBossInfo">
		<toClient desc="同步阵营BOSS的信息 ">
			<param fieldName="weiBoss" tag="1" fieldType="string" desc="魏国BOSS名称" />
			<param fieldName="weiBossKiller" tag="2" fieldType="string"
				desc="魏国BOSS击杀者，如果没有，那么就传递空字符串" />
			<param fieldName="weiBossKillerGender" tag="3" fieldType="string"
				desc="魏国BOSS击杀者的ID" />

			<param fieldName="shuBoss" tag="4" fieldType="string" desc="蜀国BOSS名称" />
			<param fieldName="shuBossKiller" tag="5" fieldType="string"
				desc="蜀国BOSS击杀者，如果没有，那么就传递空字符串" />
			<param fieldName="shuBossKillerGender" tag="6" fieldType="string"
				desc="蜀国BOSS击杀者的ID" />

			<param fieldName="wuBoss" tag="7" fieldType="string" desc="吴国BOSS名称" />
			<param fieldName="wuBossKiller" tag="8" fieldType="string"
				desc="吴国BOSS击杀者，如果没有，那么就传递空字符串" />
			<param fieldName="wuBossKillerGender" tag="9" fieldType="string"
				desc="吴国BOSS击杀者的ID" />

			<param fieldName="wuBossKillerGender" tag="10" fieldType="int32"
				desc="魏国BOSS击杀者的阵营" />
			<param fieldName="wuBossKillerGender" tag="11" fieldType="int32"
				desc="蜀国BOSS击杀者的阵营" />
			<param fieldName="wuBossKillerGender" tag="12" fieldType="int32"
				desc="吴国BOSS击杀者的阵营" />
		</toClient>
	</protocol>
	<protocol id="0x00146902" alias="syncCampBossMapSelfInfo">
		<toClient desc="同步阵营BOSS地图中自身输出和杀人信息">
			<param fieldName="damageToBoss" tag="1" fieldType="int32" desc="累计对BOSS的伤害" />
			<param fieldName="killPlayer" tag="2" fieldType="int32" desc="累计击杀敌国战力" />
			<param fieldName="gotCash" tag="3" fieldType="int32" desc="累计击杀敌得到的铜币" />
		</toClient>
	</protocol>

	<protocol id="0x00146A02" alias="selfChallengeResult">
		<toClient desc="自我挑战是否成功">
			<param fieldName="ret" tag="1" fieldType="bool"
				desc="是否成功 。true，成功 。false，失败" />
			<param fieldName="xp" tag="2" fieldType="int32" desc="挑战获得经验" />
		</toClient>
	</protocol>

	<protocol id="0x00146B02" alias="challengeMapInfo">
		<toClient desc="用户挑战类副本的信息">
			<param fieldName="type" tag="1" fieldType="int32" repeated="true"
				desc="副本类型 1 群英阵 2 群雄阵 3 南蛮入侵 4 永固南疆" />
			<param fieldName="max" tag="2" fieldType="int32" repeated="true"
				desc="最高层数/波数" />
			<param fieldName="rewardCount" tag="3" fieldType="int32"
				repeated="true" desc="副本当日领奖次数" />
			<param fieldName="rewardedTarget" tag="4" fieldType="int32"
				repeated="true" desc="已领奖的层数/波数" />
			<param fieldName="rank" tag="5" fieldType="int32" repeated="true"
				desc="我的排名" />
		</toClient>
		<toServer class="com.kueem.webgame.game.action.game.DuplicateAction"
			desc="获取用户挑战类副本的信息" method="syncChallengeMapInfo">
		</toServer>
	</protocol>

	<protocol id="0x00146C02" alias="challengeMapRank">
		<toClient desc="用户挑战类副本的排名">
			<param fieldName="currentPage" tag="1" fieldType="int32" desc="当前页数" />
			<param fieldName="totalPage" tag="2" fieldType="int32" desc="总页数" />
			<param fieldName="rank" tag="3" fieldType="int32" repeated="true"
				desc="排名" />
			<param fieldName="id" tag="4" fieldType="string" repeated="true"
				desc="id" />
			<param fieldName="name" tag="5" fieldType="string" repeated="true"
				desc="名称" />
			<param fieldName="vipLevel" tag="6" fieldType="int32" repeated="true"
				desc="vip等级" />
			<param fieldName="vipExpire" tag="7" fieldType="string"
				repeated="true" desc="vip到期时间" />
			<param fieldName="camp" tag="8" fieldType="int32" repeated="true"
				desc="阵营" />
			<param fieldName="level" tag="9" fieldType="int32" repeated="true"
				desc="等级" />
			<param fieldName="force" tag="10" fieldType="int32" repeated="true"
				desc="战力" />
			<param fieldName="maxValue" tag="11" fieldType="int32"
				repeated="true" desc="最高层数/波数" />
		</toClient>
		<toServer class="com.kueem.webgame.game.action.game.DuplicateAction"
			desc="获取用户挑战类副本的排名" method="syncChallengeMapRank">
			<param fieldName="type" tag="1" fieldType="int32"
				desc="副本类型 1 群英阵 2 群雄阵 3 南蛮入侵 4 永固南疆" />
			<param fieldName="page" tag="2" fieldType="int32" desc="页数" />
		</toServer>
	</protocol>

	<protocol id="0x00146D02" alias="rewardChallengeMap">
		<toClient desc="领取挑战类地图阶段性奖励">
			<param fieldName="ret" tag="1" fieldType="bool" desc="是否成功" />
			<param fieldName="target" tag="2" fieldType="int32" desc="领奖的层数/波数" />
			<param fieldName="type" tag="3" fieldType="int32"
				desc="副本类型 1 群英阵 2 群雄阵 3 南蛮入侵 4 永固南疆" />
		</toClient>
		<toServer class="com.kueem.webgame.game.action.game.DuplicateAction"
			desc="领奖" method="rewardChallengeMapRank">
			<param fieldName="type" tag="1" fieldType="int32"
				desc="副本类型 1 群英阵 2 群雄阵 3 南蛮入侵 4 永固南疆" />
			<param fieldName="target" tag="2" fieldType="int32" desc="领奖的层数/波数" />
		</toServer>
	</protocol>

	<protocol id="0x00146F02" alias="isBossDie">
		<toClient desc="boss是否死亡">
			<param fieldName="mapId" tag="1" fieldType="int32" desc="boss地图id" />
			<param fieldName="live" tag="2" fieldType="bool" desc="是否活阒" />
		</toClient>
		<toServer desc="boss是否死亡"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="isBossDie">
			<param fieldName="mapId" tag="1" fieldType="int32" desc="boss地图id" />
		</toServer>
	</protocol>


	<protocol id="0x00147002" alias="pvpCount">
		<toClient desc="返回用户PVP次数">
			<param fieldName="_1v1_count" tag="1" fieldType="int32" desc="1v1参与次数" />
			<param fieldName="_3v3_count" tag="2" fieldType="int32" desc="3v3参与次数" />
			<param fieldName="_cvc_count" tag="2" fieldType="int32" desc="半夜国战参与次数" />
		</toClient>
		<toServer desc="请求用户PVP次数"
			class="com.kueem.webgame.game.action.game.DuplicateAction" method="pvpCount">
		</toServer>
	</protocol>


	<protocol id="0x00147102" alias="xinshouBattleEnd">
		<toClient desc="第一场战斗副本结束">
		</toClient>
