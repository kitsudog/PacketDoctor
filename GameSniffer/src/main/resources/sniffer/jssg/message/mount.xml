<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="mount">

	<protocol id="0x00170102" alias="sync">
		<toServer desc="同步坐骑信息"
			class="com.kueem.webgame.game.action.game.MountAction" method="sync">
		</toServer>
	</protocol>

	<protocol id="0x00170202" alias="syncResult">
		<toClient desc="返回坐骑信息" proto="true"
			messageClass="com.kueem.webgame.game.protobuf.MountProtoBuf.MountProto">
		</toClient>
	</protocol>

	<protocol id="0x00170302" alias="install">
		<toServer desc="装备上坐骑"
			class="com.kueem.webgame.game.action.game.MountAction" method="install">
			<param fieldName="incId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170402" alias="uninstall">
		<toServer desc="卸载坐骑"
			class="com.kueem.webgame.game.action.game.MountAction" method="uninstall">
			<param fieldName="incId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170502" alias="free">
		<toServer desc="放生坐骑"
			class="com.kueem.webgame.game.action.game.MountAction" method="free">
			<param fieldName="incId" tag="1" fieldType="int32" />
		</toServer>

	</protocol>

	<protocol id="0x00170602" alias="feed">
		<toServer desc="喂养坐骑坐骑"
			class="com.kueem.webgame.game.action.game.MountAction" method="feed">
			<param fieldName="incId" tag="1" fieldType="int32" />
			<param fieldName="bagIndex" tag="2" fieldType="int32" repeated="true" />
			<param fieldName="numberList" tag="3" fieldType="int32"
				repeated="true" />
		</toServer>
	</protocol>

	<protocol id="0x00170702" alias="advance">
		<toServer desc="坐骑进阶"
			class="com.kueem.webgame.game.action.game.MountAction" method="advance">
			<param fieldName="incId" tag="1" fieldType="int32" />
			<param fieldName="perfect" tag="2" fieldType="bool" />
			<param fieldName="autoBuy" tag="3" fieldType="bool" />
			<param fieldName="bindFirst" tag="4" fieldType="bool" />
			<param fieldName="type" tag="5" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170802" alias="fuse">
		<toServer desc="坐骑融合"
			class="com.kueem.webgame.game.action.game.MountAction" method="fuse">
			<param fieldName="masterId" tag="1" fieldType="int32" />
			<param fieldName="slaveId" tag="2" fieldType="int32" />
			<param fieldName="attrType" tag="3" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170902" alias="huanhua">
		<toServer desc="坐骑幻化"
			class="com.kueem.webgame.game.action.game.MountAction" method="huanhua">
			<param fieldName="incId" tag="1" fieldType="int32" />
			<param fieldName="targetId" tag="2" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170A02" alias="xisui">
		<toServer desc="坐骑洗髓（刷新数值）"
			class="com.kueem.webgame.game.action.game.MountAction" method="xisui">
			<param fieldName="incId" tag="1" fieldType="int32" />
			<param fieldName="autoBuy" tag="2" fieldType="bool" />
			<param fieldName="bindFirst" tag="3" fieldType="bool" />
		</toServer>
	</protocol>

	<protocol id="0x00170B02" alias="replace">
		<toServer desc="坐骑洗髓（替换数值）"
			class="com.kueem.webgame.game.action.game.MountAction" method="replace">
			<param fieldName="incId" tag="1" fieldType="int32" />
		</toServer>
	</protocol>

	<protocol id="0x00170C02" alias="perfectAdvanceInfo">
		<toClient desc="完美进阶反馈数据">
			<param fieldName="success" tag="1" fieldType="bool" desc="是否成功" />
			<param fieldName="level" tag="2" fieldType="int32" desc="当前等级" />
			<param fieldName="perfect" tag="3" fieldType="int32" desc="完美度(整数)" />
			<param fieldName="itemCount" tag="4" fieldType="int32" desc="消耗道具数量" />
			<param fieldName="itemId" tag="5" fieldType="int32" desc="消耗道具ID" />
			<param fieldName="cash" tag="6" fieldType="int32" desc="消耗铜钱数量" />
		</toClient>
	</protocol>

	<protocol id="0x00170D02" alias="mount">
		<toServer desc="骑马"
			class="com.kueem.webgame.game.action.game.MountAction" method="mount">
		</toServer>
	</protocol>

	<protocol id="0x00170E02" alias="reincar">
		<toServer desc="坐骑转生"
			class="com.kueem.webgame.game.action.game.MountAction" method="reincar">
			<param fieldName="incId" tag="1" fieldType="int32" />
			<param fieldName="autoBuy" tag="2" fieldType="bool" />
			<param fieldName="bindFirst" tag="3" fieldType="bool" />
		</toServer>
	</protocol>

</config>