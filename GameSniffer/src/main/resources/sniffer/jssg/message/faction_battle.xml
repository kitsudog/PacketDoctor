<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config PUBLIC "//Message/" "message.dtd">
<config namespace="faction_battle">
	<protocol id="0x001C0102" alias="enterFor">
		<toServer desc="帮派战报名" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="enterFor">
		</toServer>
	</protocol>
	<protocol id="0x001C0202" alias="goInto">
		<toServer desc="进入帮派战" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="goInto">
		</toServer>
	</protocol>
	<protocol id="0x001C0302" alias="gainUserJoin">
		<toClient desc="返回报名列表">
			<param fieldName="user_id" tag="1" fieldType="string" repeated="true" />
			<param fieldName="user_name" tag="2" fieldType="string" repeated="true" />
			<param fieldName="force" tag="3" fieldType="int32" repeated="true" /><!--战斗力 -->
		</toClient>
		<toServer desc="获取报名列表" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="gainUserJoin">
		</toServer>
	</protocol>
	<protocol id="0x001C0402" alias="gainGameArrange">
		<toClient desc="返回赛事安排">
			<param fieldName="faction_name_left" tag="1" fieldType="string" repeated="true" />
			<param fieldName="faction_name_right" tag="2" fieldType="string" repeated="true" />
			<param fieldName="enter_for_num" tag="3" fieldType="int32" /><!--报名帮派个数 -->
		</toClient>
		<toServer desc="获取赛事安排" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="gainGameArrange">
		</toServer>
	</protocol>
	<protocol id="0x001C0502" alias="syncRankingsList">
		<toClient desc="返回本次帮战的个人排行(第一页)和帮派排行">
			<param fieldName="faction_rankings" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="faction_name" tag="2" fieldType="string" repeated="true" />
			<param fieldName="join_num" tag="3" fieldType="int32" repeated="true" />
			<param fieldName="rankings_score" tag="4" fieldType="int32" repeated="true" />
			<param fieldName="battle_score" tag="5" fieldType="int32" repeated="true" />

			<param fieldName="personal_rankings" tag="6" fieldType="int32" repeated="true" />
			<param fieldName="user_name" tag="7" fieldType="string" repeated="true" />
			<param fieldName="faction_name" tag="8" fieldType="string" repeated="true" />
			<param fieldName="kill_num" tag="9" fieldType="int32" repeated="true" />
			<param fieldName="assist_num" tag="10" fieldType="int32" repeated="true" />
			<param fieldName="zhangong" tag="11" fieldType="int32" repeated="true" />
			<param fieldName="total_size" tag="12" fieldType="int32" repeated="false" /><!--总条数 -->
		</toClient>
	</protocol>
	<protocol id="0x001C0602" alias="syncMyReward">
		<toClient desc="返回本次帮战的玩家奖励">
			<param fieldName="person_xp" tag="1" fieldType="int32" repeated="false" />
			<param fieldName="person_contibution" tag="2" fieldType="int32" repeated="false" />
			<param fieldName="faction_resource" tag="3" fieldType="int32" repeated="false" />
		</toClient>
	</protocol>
	<protocol id="0x001C0702" alias="notifyRoundEnd">
		<toClient desc="通知客户端本轮比赛结束">
		</toClient>
	</protocol>
	<protocol id="0x001C0802" alias="gainBattleRecord">
		<toClient desc="返回最近一次帮派战绩和帮派排名">
			<param fieldName="person_contibution" tag="1" fieldType="int32" repeated="false" /><!--个人战功 -->
			<param fieldName="kill_num" tag="2" fieldType="int32" repeated="false" /><!--击杀次数 -->
			<param fieldName="personal_rankings" tag="3" fieldType="int32" repeated="false" /><!--个人排名 -->
			<param fieldName="assist_num" tag="4" fieldType="int32" repeated="false" /><!--助攻次数 -->
			<param fieldName="faction_rankings" tag="5" fieldType="int32" repeated="true" /><!--帮派排名 -->
			<param fieldName="faction_name" tag="6" fieldType="string" repeated="true" /><!--帮派名称 -->
			<param fieldName="faction_camp" tag="7" fieldType="int32" repeated="true" /><!--国家 -->
			<param fieldName="rankings_score" tag="8" fieldType="int32" repeated="true" /><!--帮派积分 -->
			<param fieldName="battle_score" tag="9" fieldType="int32" repeated="true" /><!--帮派战分 -->
		</toClient>
		<toServer desc="获取本次帮战的个人排行" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="gainBattleRecord">
		</toServer>
	</protocol>
	<protocol id="0x001C0902" alias="gainPersonalRankings">
		<toClient desc="返回本次帮战指定页数的个人排行">
			<param fieldName="personal_rankings" tag="1" fieldType="int32" repeated="true" />
			<param fieldName="user_name" tag="2" fieldType="string" repeated="true" />
			<param fieldName="faction_name" tag="3" fieldType="string" repeated="true" />
			<param fieldName="kill_num" tag="4" fieldType="int32" repeated="true" />
			<param fieldName="assist_num" tag="5" fieldType="int32" repeated="true" />
			<param fieldName="zhangong" tag="6" fieldType="int32" repeated="true" />
		</toClient>
		<toServer desc="获取本次帮战指定页数的个人排行" class="com.kueem.webgame.game.action.game.FactionBattleAction" method="gainPersonalRankings">
			<param fieldName="page" tag="1" fieldType="int32" />
		</toServer>
	</protocol>
</config>